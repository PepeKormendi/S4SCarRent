sap.ui.define([                                                                                                                                                                                                                                                
    "sap/m/MessageToast",                                                                                                                                                                                                                                      
    'sap/ushell/services/CrossApplicationNavigation',                                                                                                                                                                                                          
	'sap/ushell/services/Container',                                                                                                                                                                                                                              
], function(MessageToast,CrossApplicationNavigation,Container) {                                                                                                                                                                                               
    'use strict';                                                                                                                                                                                                                                              
                                                                                                                                                                                                                                                               
    return {                                                                                                                                                                                                                                                   
        navigate: function(oEvent) {                                                                                                                                                                                                                           
            var oCrossAppNav = Container.getService("CrossApplicationNavigation");&nbsp;                                                                                                                                                                       
            // trigger navigation                                                                                                                                                                                                                              
            oCrossAppNav.toExternal({                                                                                                                                                                                                                          
                target : { semanticObject : "zcustomer", action : "display" }                                                                                                                                                                                  
            });&nbsp;&nbsp;                                                                                                                                                                                                                                    
        }                                                                                                                                                                                                                                                      
    };                                                                                                                                                                                                                                                         
});                                                                                                                                                                                                                                                            